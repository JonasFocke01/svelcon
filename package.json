{
  "name": "@jonas_focke/svelcon",
  "version": "0.2.6",
  "custom": {
    "node-version": "16.15.0"
  },
  "scripts": {
    "dev": "env-cmd svelte-kit dev",
    "build": "env-cmd svelte-kit build",
    "preview": "env-cmd svelte-kit preview",
    "test:once": "jest src --coverage --collectCoverageFrom=src/lib/**/*.svelte --silent",
    "test": "jest src --watch --only-changed --silent",
    "bump": "npm version --no-git-tag-version patch"
  },
  "setupFilesAfterEnv": [
    "@testing-library/jest-dom/extend-expect"
  ],
  "exports": {
    ".": "./src/index.js",
    "./Checkbox": "./Checkbox.svelte",
    "./Date": "./Date.svelte",
    "./Dropdown": "./Dropdown.svelte",
    "./NormalButton": "./NormalButton.svelte",
    "./Number": "./Number.svelte",
    "./Password": "./Password.svelte",
    "./RadioGroup": "./RadioGroup.svelte",
    "./SmallButton": "./SmallButton.svelte",
    "./TextArea": "./TextArea.svelte",
    "./Card": "./Card.svelte",
    "./InplacePopup": "./InplacePopup.svelte",
    "./List": "./List.svelte",
    "./MediaQuery": "./MediaQuery.svelte",
    "./Table": "./Table.svelte",
    "./Text": "./Text.svelte"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0-next.54",
    "@sveltejs/kit": "v1.0.0-next.347",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.3",
    "@types/babel__core": "^7.1.19",
    "@zerodevx/svelte-toast": "^0.7.1",
    "autoprefixer": "^10.4.4",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-fetch-mock": "^3.0.3",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.11",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^28.0.5",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "dependencies": {
    "date-fns": "^2.28.0",
    "env-cmd": "^10.1.0",
    "js-sha256": "^0.9.0",
    "node-fetch": "^3.2.6"
  }
}
